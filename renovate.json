{
    "baseBranches": ["develop"],
    "branchConcurrentLimit": 10,
    "commitMessageAction": "Upgrade",
    "commitMessagePrefix": "chore:",
    "commitMessageTopic": "{{depName}}",
    "extends": ["config:base"],
    "labels": ["dependencies"],
    "packageRules": [
        {
            "matchUpdateTypes": ["patch", "pin"],
            "automerge": true
        }
    ],
    "prCreation": "not-pending",
    "prConcurrentLimit": 3,
    "prHourlyLimit": 1,
    "rebaseWhen": "auto",
    "reviewers": ["djdembeck"],
    "stabilityDays": 3,
    "timezone": "Etc/UTC",
    "vulnerabilityAlerts": {
        "labels": ["security"]
    }
}
